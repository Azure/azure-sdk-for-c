  "ignoreWords": [
    "azsdk",
    "MSVC",
    "CMAKE",
    "Xclang",
    "fcomment",
    "Wlogical",
    "favicon",
    "includedbygraph",
    "generatedby",
    "jquery",
    "mathjax",
    "sidetoc",
    "structs",
    "structlist",
    "structindex",
    "structs",
    "enumvalues",
    "dynsections",
    "xlink",
    "mkdir",
    "docfx",
    "untracked",
    "nolog",
    "gcovr",
    "lcov",
    "eabi",
    "POSIX",
    "noprc",
    "paho",
    "mqtt",
    "cmocka",
    "vcpkg",
    "deps",
    "SPDX",
    "Cobertura",
    "mspremier",
    "ctest",
    "winssl",
    "pwsh",
    "githubio",
    "moxygen"
  ],
  "allowCompoundWords": true

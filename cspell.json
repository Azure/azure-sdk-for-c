{
  "version": "0.1",
  "language": "en",
  "languageId": "c,css",
  "dictionaries": [
    "powershell",
    "css"
  ],
  "ignorePaths": [
    "vcpkg/**",
    "*.exe",
    "*.a",
    "*.lib",
    "build/**",
    "*.png",
    "*.svg",
    "eng/docs/api/assets/style.css",
